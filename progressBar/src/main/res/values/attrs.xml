<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ArcCircleProgressBar">
        <attr name="progress" format="float"/>
        <attr name="roundTips" format="boolean"/>

        <attr name="indicatorWidth" format="dimension"/>
        <attr name="indicatorStartAngle" format="float"/>
        <attr name="indicatorEndAngle" format="float"/>
        <attr name="indicatorColor" format="color"/>
        <attr name="indicatorShowGradient" format="boolean"/>
        <attr name="indicatorGradientAngle" format="float"/>
        <attr name="indicatorGradientWidth" format="dimension"/>
        <attr name="indicatorGradientColors" format="reference"/>
        <attr name="indicatorGradientPositions" format="reference"/>
        <attr name="indicatorGradientTileMode" format="integer">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
            <enum name="decal" value="3"/>
        </attr>

        <attr name="indicatorDrawShadow" format="boolean"/>
        <attr name="indicatorShadowDrawGradient" format="boolean"/>
        <attr name="indicatorShadowGradientAngle" format="float"/>
        <attr name="indicatorShadowGradientWidth" format="dimension"/>
        <attr name="indicatorShadowColors" format="reference"/>
        <attr name="indicatorShadowGradientPositions" format="reference"/>
        <attr name="indicatorShadowBlurValue" format="float"/>
        <attr name="indicatorShadowGradientTileMode" format="integer">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
            <enum name="decal" value="3"/>
        </attr>



        <attr name="canalWidth" format="dimension"/>
        <attr name="canalStartAngle" format="float"/>
        <attr name="canalEndAngle" format="float"/>
        <attr name="canalColor" format="color"/>
        <attr name="canalShowGradient" format="boolean"/>
        <attr name="canalGradientAngle" format="float"/>
        <attr name="canalGradientWidth" format="dimension"/>
        <attr name="canalGradientColors" format="reference"/>
        <attr name="canalGradientPositions" format="reference"/>
        <attr name="canalGradientTileMode" format="integer">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
            <enum name="decal" value="3"/>
        </attr>

        <attr name="canalDrawShadow" format="boolean"/>
        <attr name="canalShadowDrawGradient" format="boolean"/>
        <attr name="canalShadowGradientAngle" format="float"/>
        <attr name="canalShadowGradientWidth" format="dimension"/>
        <attr name="canalShadowColors" format="reference"/>
        <attr name="canalShadowGradientPositions" format="reference"/>
        <attr name="canalShadowBlurValue" format="float"/>
        <attr name="canalShadowGradientTileMode" format="integer">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
            <enum name="decal" value="3"/>
        </attr>

    </declare-styleable>
</resources>